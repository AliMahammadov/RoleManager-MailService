@using Pronia.Models;

@{
    ViewData["Title"] = "Create";
}
@model Category
<div class="container">


    <form method="post">
        <div class="form-group">
            <label  asp-for="Name"></label>
            <input class="btn btn-secondary btn-sm" asp-for="Name" class="form-control ">
            <span asp-validation-for="Name"></span>
        </div>
        <div class="form-group">
            <label asp-for="ImgUrl"></label>
            <input class="btn btn-secondary btn-sm"  asp-for="ImgUrl" class=" form-control">
        </div>


        <button a class="btn btn-info" class="btn btn-default">Submit</button>
      
    </form>
</div>

